
    <div class="container">
        <h1>Manage Profiles</h1>
        <table>
            <thead>
                <tr>
                    <th>Username</th>
                    <th>Email</th>
                    <th>Phone</th>
                    <th>Actions</th>
                </tr>
            </thead>
            <tbody>
                <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.username %></td>
                        <td><%= user.email %></td>
                        <td><%= user.phone %></td>
                        <td>
                            <form action="/superadmin/manage_profiles" method="POST">
                                <input type="hidden" name="username" value="<%= user.username %>">
                                <input type="text" name="email" value="<%= user.email %>" required>
                                <input type="text" name="phone" value="<%= user.phone %>" required>
                                <button type="submit">Update</button>
                            </form>
                        </td>
                    </tr>
                <% }) %>
            </tbody>
        </table>
    </div>
   

